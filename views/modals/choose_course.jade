.modal.fade#course_alert(tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true")
    .modal-dialog
        .modal-content
            .modal-header
                button.close_btn(type="button" class="close" data-dismiss="modal" style="outline:none")
                    img(style='vertical-align: middle', src='#{data.cdnUrl}/static/images/blackcancelicon.png')
                h4.modal-title
            .modal-body.chosse_course
                button.chosse_click_btn.all(style='outline:none' skill-type="all") 全部
                button.chosse_click_btn.music(style='outline:none' skill-type="music") 器乐
                button.chosse_click_btn.dance1(style='outline:none' skill-type="dance") 舞蹈
                button.chosse_click_btn.art(style='outline:none' skill-type="art") 美术
                button.chosse_click_btn.media(style='outline:none' skill-type="media_arts") 媒体艺术
                button.chosse_click_btn.wushu(style='outline:none' skill-type="gongfu") 武术
                button.chosse_click_btn.chess(style='outline:none' skill-type="chess") 棋艺
                span.search_background
                    .input-group
                        input.form-control.searchcou(type='text' style='outline:none')
                    button.btn.btn-info.searchcou(style='outline:none;position:absolute;right:-100px;width:60px') 搜索
                #carousel-course.carousel.slide.scrollview
                    .carousel-inner.aaaabox(role="listbox")
                    a.left.last_page(href="#carousel-course" role="button" data-slide="prev")
                        img(style='vertical-align: middle;width:100%', src='#{data.cdnUrl}/static/images/leftslide.png')
                        //<span class="glyphicon glyphicon-chevron-left"></span>
                        //<span class="sr-only">Previous</span>
                    a.right.next_page(href="#carousel-course" role="button" data-slide="next")
                        img(style='vertical-align: middle;width:100%', src='#{data.cdnUrl}/static/images/rightslide.png')
                        //<span class="glyphicon glyphicon-chevron-right"></span>
                         //<span class="sr-only">Next</span>
                        //data-dismiss="modal"
                button.btn.btn-success.sure_c(data-dismiss="modal" style='outline:none') 确定
        //button.select_btn.art 美术
        //button.select_btn.media 媒体艺术
        //button.select_btn.wushu 武术
        //button.select_btn.chess 棋艺

